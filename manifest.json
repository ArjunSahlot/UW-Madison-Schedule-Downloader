{
  "name": "UW-Madison Schedule Download",
  "description": "Download your UW-Madison schedule as a file.",
  "version": "1.0.1",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://my.wisc.edu/portal/p/class-schedule*/*"],
      "js": ["content-script.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [ "<all_urls>" ],
      "resources": [
        "scripts/*",
        "style/*"
      ]
    }
  ],
  "manifest_version": 3
}